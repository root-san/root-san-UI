{
  "name": "root-san-ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"next dev\" \"yarn start-mock\"",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prettier": "prettier --write --ignore-unknown .",
    "prettier:check": "prettier --check --ignore-unknown .",
    "test": "yarn prettier:check && yarn lint",
    "gen-api": "openapi-generator-cli generate -g typescript-fetch -i https://raw.githubusercontent.com/root-san/root-san/main/docs/openapi.yaml -o ./libs/apis/generated/",
    "postinstall": "yarn gen-api",
    "start-mock": "prism mock -p 4010 -d https://raw.githubusercontent.com/root-san/root-san/main/docs/openapi.yaml",
    "clean": "rimraf /libs/apis/generated"
  },
  "dependencies": {
    "@ducanh2912/next-pwa": "^7.1.1",
    "next": "13.0.6",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "swr": "^2.0.0"
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.5.2",
    "@stoplight/prism-cli": "^4.10.5",
    "@types/node": "18.11.15",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "autoprefixer": "^10.4.13",
    "concurrently": "^7.6.0",
    "daisyui": "^2.43.2",
    "eslint": "8.29.0",
    "eslint-config-next": "13.0.6",
    "eslint-config-prettier": "^8.5.0",
    "postcss": "^8.4.20",
    "prettier": "^2.8.1",
    "prettier-plugin-tailwindcss": "^0.2.1",
    "rimraf": "^3.0.2",
    "tailwindcss": "^3.2.4",
    "typescript": "4.9.4"
  }
}
